#: package/contents/ui/main.qml:116
#, kde-format
msgid "Minimize All Applet Active Indicator"
msgstr "Indicador de miniaplicación «Minimizar todo» activo"

#: package/contents/ui/MinimizeAllController.qml:17
#, kde-format
msgctxt "@action:button"
msgid "Restore All Minimized Windows"
msgstr "Restaurar todas las ventanas minimizadas"

#: package/contents/ui/MinimizeAllController.qml:18
#, kde-format
msgctxt "@action:button"
msgid "Minimize All Windows"
msgstr "Minimizar todas las ventanas"

#: package/contents/ui/MinimizeAllController.qml:20
#, kde-format
msgctxt "@info:tooltip"
msgid "Restores the previously minimized windows"
msgstr "Restaura las ventana anteriormente minimizadas"

#: package/contents/ui/MinimizeAllController.qml:21
#, kde-format
msgctxt "@info:tooltip"
msgid "Shows the Desktop by minimizing all windows"
msgstr "Muestra el escritorio minimizando todas las ventanas"

#: package/contents/ui/PeekController.qml:14
#, kde-format
msgctxt "@action:button"
msgid "Peek at Desktop"
msgstr "Dar un vistazo al escritorio"

#: package/contents/ui/PeekController.qml:15
#, kde-format
msgctxt "@action:button"
msgid "Stop Peeking at Desktop"
msgstr "Dejar de mirar al escritorio"

#: package/contents/ui/PeekController.qml:17
#, kde-format
msgctxt "@info:tooltip"
msgid "Moves windows back to their original positions"
msgstr "Devuelve las ventanas a su posición original"

#: package/contents/ui/PeekController.qml:18
#, kde-format
msgctxt "@info:tooltip"
msgid "Temporarily shows the desktop by moving windows away"
msgstr "Muestra el escritorio temporalmente apartando las ventanas"

#~ msgid "Show Desktop"
#~ msgstr "Mostrar el escritorio"

#~ msgid "Show the Lingmo desktop"
#~ msgstr "Mostrar el escritorio de Lingmo"

#~ msgid "Minimize all open windows and show the Desktop"
#~ msgstr "Minimizar todas las ventanas abiertas y mostrar el escritorio"
